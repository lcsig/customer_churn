# Copy this file to ".env" and fill in the values.
#
# If MLFLOW_USERNAME and MLFLOW_PASSWORD are provided, the training script will
# inject them into the tracking URI:
#   https://<user>:<pass>@mlflow-server.example.com
#
# Otherwise, it will use MLFLOW_TRACKING_URI as-is.

# Base tracking URI (no credentials required)
export MLFLOW_TRACKING_URI=http://127.0.0.1:5000

# Optional basic-auth credentials (leave empty if not needed)
export MLFLOW_USERNAME=
export MLFLOW_PASSWORD=
